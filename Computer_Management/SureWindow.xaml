        <Label Name="mainText" Content="MainText" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="309" FontSize="14"/>
        <Button Name="yesBTN" Content="Yes" HorizontalAlignment="Left" Margin="163,65,0,0" VerticalAlignment="Top" Width="75" Click="yesBTN_Click"/>
        <Button Name="noBTN" Content="No" HorizontalAlignment="Left" Margin="253,65,0,0" VerticalAlignment="Top" Width="75" Click="noBTN_Click"/>

    </Grid>
</Window>
