<Window x:Class="Computer_Management.SureWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Computer_Management"
        mc:Ignorable="d"
        Icon="Icons\SureWindow_icon.png"
        Name="surewindowWindow"
        Title="Window title" Height="135" Width="357" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Background="#FFFDFDFD" Foreground="Black" KeyDown="Window_KeyDown" Closed="surewindowWindow_Closed">
    <Grid>
        <Label Name="mainText" Content="MainText" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="309" FontSize="14"/>
        <StackPanel HorizontalAlignment="Right" VerticalAlignment="Bottom" Orientation="Horizontal" Margin="10">
            <Button Name="yesBTN" Content="Yes" Width="75" Click="yesBTN_Click"/>
            <Button Name="noBTN" Content="No" Width="75" Margin="15,0,0,0" Click="noBTN_Click"/>
        </StackPanel>
    </Grid>
</Window>
